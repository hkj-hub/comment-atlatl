{
  "extends": "@comment-atlatl/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "next-env.d.ts",
    "next.config.js",
    "../../.next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "eslint.config.mjs"
  ]
}
